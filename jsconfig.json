{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@assets": ["src/assets/index.js"],
      "@atoms/*": ["src/components/atoms/*"],
      "@atoms": ["src/components/atoms/index.js"],
      "@molecules/*": ["src/components/molecules/*"],
      "@molecules": ["src/components/molecules/index.js"],
      "@organisms/*": ["src/components/organisms/*"],
      "@organisms": ["src/components/organisms/index.js"],
      "@templates/*": ["src/components/templates/*"],
      "@templates": ["src/components/templates/index.js"],
      "@pages/*": ["src/components/pages/*"],
      "@pages": ["src/components/pages/index.js"],
      "@components/*": ["src/components/*"],
      "@components": ["src/components/index.js"],
      "@config/*": ["src/config/*"],
      "@config": ["src/config/index.js"],
      "@locale/*": ["src/locale/*"],
      "@routes/*": ["src/routes/*"],
      "@routes": ["src/routes/index.js"],
      "@store/*": ["src/store/*"],
      "@store": ["src/store/index.js"],
      "@utils/*": ["src/utils/*"],
      "@hocs/*": ["src/hocs/*"],
      "@hocs": ["src/hocs/index.js"],
      "@hooks/*": ["src/hooks/*"],
      "@hooks": ["src/hooks/index.js"],
      "@services/*": ["src/services/*"],
      "@services": ["src/services/index.js"],
      "@styles/*": ["src/styles/*"],
      "@constants": ["src/constants/index.js"],
      "@constants/*": ["src/constants/*"],
      "@root/*": ["src/*"],
      "@root": ["src/index.js"]
    }
  },
  "exclude": ["node_modules"]
}
